<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.motoyasu.needforcommunicating" >
    <uses-permission android:name="android.permission.CALL_PHONE"></uses-permission>
    <uses-permission android:name="ANDROID.PERMISSION.SEND_SMS"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/f1"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Friends"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".UserInterface"
            android:label="@string/title_activity_user_interface" >
        </activity>
        <activity
            android:name=".Telephone"
            android:label="@string/title_activity_telephone" >
        </activity>
        <activity
            android:name=".ShortMessage"
            android:label="@string/title_activity_short_message" >
        </activity>
    </application>

</manifest>
