<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.motoyasu.needforcommunicating.Telephone">

    <TextView android:text="请输入号码"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView1"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/editText"
        android:layout_alignStart="@+id/editText"
        android:layout_marginTop="24dp" />

    <EditText
        android:layout_width="300dp"
        android:layout_height="80dp"
        android:textSize="30dp"
        android:id="@+id/editText"
        android:layout_below="@+id/textView1"
        android:layout_centerHorizontal="true" />

    <RelativeLayout
        android:layout_width="330dp"
        android:layout_height="330dp"
        android:id="@+id/RL1"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <Button
            android:background="#00ffffff"
            android:layout_width="108dp"
            android:layout_height="70dp"
            android:text="1"
            android:id="@+id/button1"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <Button
            android:background="#00ffffff"
            android:layout_width="108dp"
            android:layout_height="wrap_content"
            android:text="2"
            android:id="@+id/button2"
            android:layout_alignBottom="@+id/button1"
            android:layout_toRightOf="@+id/button1"
            android:layout_toEndOf="@+id/button1"
            android:layout_alignParentTop="true" />

        <Button
            android:background="#00ffffff"
            android:layout_width="108dp"
            android:layout_height="wrap_content"
            android:text="3"
            android:id="@+id/button3"
            android:layout_alignBottom="@+id/button2"
            android:layout_toRightOf="@+id/button2"
            android:layout_toEndOf="@+id/button2"
            android:layout_alignParentTop="true" />

        <Button
            android:background="#00ffffff"
            android:layout_width="108dp"
            android:layout_height="70dp"
            android:text="4"
            android:id="@+id/button4"
            android:layout_below="@+id/button1"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <Button
            android:background="#00ffffff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="5"
            android:id="@+id/button5"
            android:layout_alignBottom="@+id/button4"
            android:layout_below="@+id/button2"
            android:layout_alignLeft="@+id/button2"
            android:layout_alignStart="@+id/button2"
            android:layout_alignRight="@+id/button2"
            android:layout_alignEnd="@+id/button2" />

        <Button
            android:background="#00ffffff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="6"
            android:id="@+id/button6"
            android:layout_alignBottom="@+id/button5"
            android:layout_below="@+id/button3"
            android:layout_alignLeft="@+id/button3"
            android:layout_alignStart="@+id/button3"
            android:layout_alignRight="@+id/button3"
            android:layout_alignEnd="@+id/button3" />

        <Button
            android:background="#00ffffff"
            android:layout_width="108dp"
            android:layout_height="70dp"
            android:text="7"
            android:id="@+id/button7"
            android:layout_below="@+id/button4"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <Button
            android:background="#00ffffff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="8"
            android:id="@+id/button8"
            android:layout_alignTop="@+id/button7"
            android:layout_above="@+id/button10"
            android:layout_alignLeft="@+id/button5"
            android:layout_alignStart="@+id/button5"
            android:layout_toStartOf="@+id/button9"
            android:layout_toLeftOf="@+id/button9" />

        <Button
            android:background="#00ffffff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="9"
            android:id="@+id/button9"
            android:layout_alignBottom="@+id/button8"
            android:layout_alignTop="@+id/button8"
            android:layout_toRightOf="@+id/button5"
            android:layout_alignRight="@+id/button6"
            android:layout_alignEnd="@+id/button6" />

        <Button
            android:background="#00ffffff"
            android:layout_width="108dp"
            android:layout_height="70dp"
            android:text="*"
            android:textSize="30dp"
            android:id="@+id/button10"
            android:layout_below="@+id/button7"
            android:layout_toLeftOf="@+id/button8"
            android:layout_toStartOf="@+id/button8" />

        <Button
            android:background="#00ffffff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:id="@+id/button11"
            android:layout_alignBottom="@+id/button10"
            android:layout_alignRight="@+id/button8"
            android:layout_alignEnd="@+id/button8"
            android:layout_below="@+id/button7"
            android:layout_alignLeft="@+id/button8"
            android:layout_alignStart="@+id/button8" />

        <Button
            android:background="#00ffffff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="#"
            android:textSize="25dp"
            android:id="@+id/button12"
            android:layout_alignBottom="@+id/button10"
            android:layout_below="@+id/button8"
            android:layout_toRightOf="@+id/button8"
            android:layout_alignRight="@+id/button9"
            android:layout_alignEnd="@+id/button9" />

        <Button
            android:background="#00ffffff"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:text="拨号"
            android:id="@+id/dail"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/button10"
            android:layout_toLeftOf="@+id/button12"
            android:layout_toStartOf="@+id/button12" />

        <Button
            android:background="#00ffffff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="&lt;&lt;DEL"
            android:id="@+id/button13"
            android:layout_alignTop="@+id/dail"
            android:layout_toRightOf="@+id/button11"
            android:layout_alignParentBottom="true"
            android:layout_alignRight="@+id/button12"
            android:layout_alignEnd="@+id/button12" />
    </RelativeLayout>

</RelativeLayout>
